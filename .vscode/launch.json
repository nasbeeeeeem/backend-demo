{
  "configurations": [
    {
      "name": "Cloud Run: Run/backend-demo",
      "type": "cloudcode.cloudrun",
      "request": "launch",
      "build": {
        "docker": {
          "path": "Dockerfile",
          "target": "Dockerfile",
          "buildArgs": {}
        }
      },
      "image": "backend-demo",
      "service": {
        "name": "backend-demo",
        "containerPort": 8080,
        "resources": {
          "limits": {
            "memory": "256Mi"
          }
        }
      },
      "target": {
        "minikube": {}
      },
      "watch": false,
      "externalPortForward": 3000
    }
  ]
}